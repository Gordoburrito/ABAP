{
  "data_source": "Ford",
  "processing": {
    "batch_size": 100,
    "max_retries": 3,
    "timeout_seconds": 300
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "paths": {
    "raw_data": "Ford/data/raw",
    "processed_data": "Ford/data/processed", 
    "results": "Ford/data/results",
    "samples": "Ford/data/samples"
  },
  "features": {
    "enable_preprocessing": true,
    "enable_validation": true,
    "enable_postprocessing": true
  }
}